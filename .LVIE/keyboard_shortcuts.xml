<Keyboard>
    <key value="o">
        <binding action="editor.file.open">
            <ctrl/>
        </binding>
    </key>
    <key value="e">
        <binding action="editor.file.close">
            <ctrl/><shift/>
        </binding>
    </key>
    <key value="r">
        <binding action="editor.image.rotate-90-deg">
            <ctrl/><shift/>
        </binding>
    </key>
</Keyboard>